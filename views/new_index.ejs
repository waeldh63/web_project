<script>
  $(document).ready(function() {
    
    $('#download').click(function(){
      $('html, body').animate({scrollTop: $("#footer").offset().top}, 800);
    });

    $(".auto_com").autocomplete({
      source: async function(request, response) {
        let data = await fetch(`http://localhost:3001/search?term=${request.term}`)
          .then(results => results.json())
          .then(results => results.map(result => {
            return { label: result.name, value: result.name, id: result._id };
          }));
          response(data);
      },
      minLength: 2,
        select: function(event, ui) {
          console.log(ui.item);
        }
    });

    $( "#sendEmail" ).submit(function(event) {
				event.preventDefault();

				$.ajax({
					type: 'POST',
					url: '/send-email',
					data: $('#sendEmail').serialize(),
					dataType: "json",
					success: function(response){
						$('#sendEmail')[0].reset();
            document.getElementById("sent-message").style.display = "block";
						document.getElementById("sent-message").innerHTML=response.Success;
         	},
         	error: function() {
            document.getElementById("error-msg").style.display = "block";
						document.getElementById("error-msg").innerHTML=response.Failed;
         	}
    		})
  	});

  });
</script>

